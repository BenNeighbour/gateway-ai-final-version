  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Build Demo",
      "preLaunchTask": "package: debug",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/demo",
      "runtimeExecutable": "netlify",
      "runtimeArgs": ["build"],
      "outFiles": ["${workspaceFolder}/lib/**/*.js"]
    }
  ]
}
